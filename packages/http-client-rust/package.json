{
  "name": "@snps/http-client-rust",
  "version": "0.2.1",
  "description": "Zero-dependency high-performance HTTP client built with Rust - Blazing fast with 10x performance improvement",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "snps-http-client-rust",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "aarch64-apple-darwin",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "cargo test && node --test tests/*.test.js",
    "bench": "cargo bench",
    "version": "napi version",
    "clean": "cargo clean"
  },
  "keywords": [
    "http",
    "https",
    "client",
    "rust",
    "performance",
    "zero-dependency",
    "napi",
    "nodejs",
    "typescript",
    "synapse"
  ],
  "author": "Synapse Framework Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/synapse-framework/synapse.git",
    "directory": "packages/http-client-rust"
  },
  "bugs": {
    "url": "https://github.com/synapse-framework/synapse/issues"
  },
  "homepage": "https://synapse.dev",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "optionalDependencies": {
    "@snps/http-client-rust-win32-x64-msvc": "0.2.1",
    "@snps/http-client-rust-darwin-x64": "0.2.1",
    "@snps/http-client-rust-linux-x64-gnu": "0.2.1",
    "@snps/http-client-rust-linux-x64-musl": "0.2.1",
    "@snps/http-client-rust-linux-arm64-gnu": "0.2.1",
    "@snps/http-client-rust-darwin-arm64": "0.2.1",
    "@snps/http-client-rust-linux-arm-gnueabihf": "0.2.1"
  }
}
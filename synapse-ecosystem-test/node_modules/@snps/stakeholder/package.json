{
  "name": "@snps/stakeholder",
  "version": "1.0.2",
  "description": "Comprehensive stakeholder reporting package for generating beautiful documents and reports covering tests, coverage, performance, lighthouse reports, and more",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "stakeholder",
    "reporting",
    "documentation",
    "metrics",
    "analytics",
    "dashboard",
    "executive",
    "compliance",
    "security",
    "performance",
    "coverage",
    "lighthouse",
    "synapse"
  ],
  "author": "Synapse Framework Team",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.19.19",
    "@typescript-eslint/eslint-plugin": "8.0.0",
    "@typescript-eslint/parser": "8.0.0",
    "eslint": "9.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@playwright/test": "^1.56.0",
    "chart.js": "^4.0.0",
    "exceljs": "^4.0.0",
    "jspdf": "^2.0.0",
    "playwright": "^1.56.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kluth/synapse-stakeholder.git"
  },
  "bugs": {
    "url": "https://github.com/kluth/synapse-stakeholder/issues"
  },
  "homepage": "https://github.com/kluth/synapse-stakeholder#readme",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "prebuild": "pnpm run clean"
  }
}